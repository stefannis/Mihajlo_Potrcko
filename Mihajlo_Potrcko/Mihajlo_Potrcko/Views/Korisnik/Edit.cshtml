@using Mihajlo_Potrcko.Models
@model Mihajlo_Potrcko.Components.ViewDataContainer

@{
    ViewBag.Title = "Korisnik";
    Korisnik pageModel = Model.PageModel;
    if (pageModel == null)
    {
        pageModel = new Korisnik();
    }

    <h2>Izmeni podatke o korisniku</h2>
    Html.RenderPartial("_PostForm", pageModel);
}